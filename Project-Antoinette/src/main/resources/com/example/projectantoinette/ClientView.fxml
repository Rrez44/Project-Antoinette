<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.projectantoinette.ClientController">
    <TextArea fx:id="messageArea" editable="false" wrapText="true" />
    <TextField fx:id="inputField" promptText="Enter your message here..." />
    <Button text="Send" onAction="#handleSendMessage" />
</VBox>
